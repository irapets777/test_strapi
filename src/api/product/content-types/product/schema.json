{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "products"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Tite": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "Price": {
      "type": "decimal"
    },
    "Currency": {
      "type": "enumeration",
      "enum": [
        "UAH",
        "USD",
        "PLN"
      ]
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "products"
    }
  }
}
